<ModelBehaviors>
    <!--
    Template for rigging laptop keys

    Define one level up:
            - SIDE_NODE_GROUP   <no default>    side (CPT or FO)

    Main Parameters:
            - KEY               <no default>    Node-ID suffix of the key; also used in the tooltip and H event
    -->
    <Template Name="FBW_A380X_Laptop_Keyboard_Key_Template">
        <UseTemplate Name="FBW_A380X_BacklightIndicator_Button_Template">
            <NODE_ID>PUSH_KBD_#SIDE_NODE_GROUP#_#KEY#</NODE_ID>
            <BACKLIGHT_NODE_ID>PUSH_KBD_#SIDE_NODE_GROUP#_#KEY#_SEQ1</BACKLIGHT_NODE_ID>
            <ANIM_NAME>PUSH_KBD_#SIDE_NODE_GROUP#_ANIM_#KEY#</ANIM_NAME>

            <SEQ1_CODE>(L:#PLANE_NAME#_KBD_#SIDE_NODE_GROUP#_#KEY#, Bool)</SEQ1_CODE>
            <LEFT_SINGLE_CODE>1 (&gt;L:#PLANE_NAME#_KBD_#SIDE_NODE_GROUP#_#KEY#, Bool)</LEFT_SINGLE_CODE>

            <DISABLE_INDICATOR />

            <BUTTON_CODE>(&gt;H:A380X_LAPTOP_KEYBOARD_#SIDE_NODE_GROUP#_#KEY)</BUTTON_CODE>
        </UseTemplate>
    </Template>

